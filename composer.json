{
    "name": "innmind/stack-trace",
    "type": "library",
    "description": "Tool to inspect exception stack trace",
    "keywords": ["inspect", "exception", "stack trace"],
    "homepage": "http://github.com/Innmind/StackTrace",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/StackTrace/issues"
    },
    "require": {
        "php": "~8.4",
        "innmind/immutable": "dev-next",
        "innmind/url": "dev-next",
        "innmind/graphviz": "dev-next",
        "innmind/filesystem": "dev-next",
        "innmind/colour": "dev-next",
        "innmind/io": "dev-next",
        "innmind/media-type": "dev-next",
        "innmind/validation": "dev-next",
        "innmind/ip": "dev-next",
        "innmind/time-continuum": "dev-next"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\StackTrace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\StackTrace\\": "tests/"
        }
    },
    "require-dev": {
        "innmind/static-analysis": "~1.2.1",
        "innmind/black-box": "^6.4.1",
        "innmind/coding-standard": "~2.0"
    }
}
