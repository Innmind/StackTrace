{
    "name": "innmind/stack-trace",
    "type": "library",
    "description": "Tool to inspect exception stack trace",
    "keywords": ["inspect", "exception", "stack trace"],
    "homepage": "http://github.com/Innmind/StackTrace",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/StackTrace/issues"
    },
    "require": {
        "php": "~8.2",
        "innmind/immutable": "~4.1|~5.0",
        "innmind/url": "~4.0",
        "innmind/graphviz": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\StackTrace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\StackTrace\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~10.2",
        "vimeo/psalm": "~5.12",
        "innmind/black-box": "~5.5",
        "innmind/coding-standard": "~2.0"
    }
}
